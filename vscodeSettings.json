{
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.semanticHighlighting.enabled": true,
    "editor.suggest.insertMode": "replace",
    "editor.wordBasedSuggestions": false,
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.semanticHighlighting.enabled": true,
    "editor.suggest.insertMode": "replace",
    "editor.wordBasedSuggestions": false,
  },
  "[haskell]": {},
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.maxTokenizationLineLength": 2500,
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.maxTokenizationLineLength": 2500,
    "editor.suggest.insertMode": "replace",
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[prolog]": {},
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.maxTokenizationLineLength": 2500,
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.maxTokenizationLineLength": 2500,
  },
  "advancedNewFile.convenienceOptions": [
    "last",
    "root"
  ],
  "advancedNewFile.exclude": {
    "node_modules": true,
    "build": true,
    "dist": true,
    "public": true
  },
  "breadcrumbs.enabled": false,
  "C_Cpp.clang_format_sortIncludes": true,
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.vcFormat.indent.preserveComments": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
  "C_Cpp.vcFormat.newLine.scopeBracesOnSeparateLines": true,
  "customizeUI.listRowHeight": 27,
  "customizeUI.stylesheet": {
    ".monaco-workbench .part > .content .folder-icon": "font-weight: bold;",
    ".monaco-workbench .part.sidebar>.title>.title-label h2": "display: none;",
  },
  "diffEditor.wordWrap": "on",
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "'BitstreamVeraSansMono NF','SourceCodePro NF','Cascadia Code', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontWeight": 350,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 30,
  "editor.linkedEditing": true,
  "editor.minimap.renderCharacters": false,
  "editor.padding.top": 15,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "none",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.semanticTokenColorCustomizations": {
    "[One Monokai]": {
      "enabled": true,
      "rules": {
        "class": {
          "foreground": "#d19a66",
          "bold": true
        },
        "enumMember": {
          "foreground": "#ab38cd",
          "italic": true
        },
        "function": {
          "foreground": "#61afef",
          "italic": true
        },
        "interface": {
          "foreground": "#d19a66",
          "bold": true
        },
        "method": {
          "foreground": "#61afef",
          "italic": true,
          "bold": true
        },
        "property": {
          "foreground": "#43b6b9",
          "italic": true
        },
        "type": {
          "foreground": "#E5C07B"
        },
        "typeParameter": {
          "foreground": "#E5C07B",
          "bold": true
        },
        "variable": "#43b6b9",
      }
    }
  },
  "editor.smoothScrolling": true,
  "editor.suggest.showWords": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[One Monokai]": {
      "functions": "#61afef",
      "keywords": "#C678DD",
      "numbers": "#C678DD",
      "strings": "#98C379",
      "types": "#E5C07B",
      "variables": "#43b6b9",
      "textMateRules": [
        {
          "scope": [
            "constant.language"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#D19A66",
          }
        },
        {
          "scope": [
            "entity.name.function",
            "support.function.js",
            "support.type.property-name.css"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#61afef",
          }
        },
        {
          "scope": [
            "punctuation.separator.key-value",
            "storage.type.function.arrow.js"
          ],
          "settings": {
            "foreground": "#E06C75"
          }
        },
        {
          "scope": [
            "support.class.component"
          ],
          "settings": {
            "foreground": "#E5C07B"
          }
        },
        {
          "scope": [
            "punctuation.definition.typeparameters",
            "keyword.operator.expression",
            "storage.modifier",
            "storage.type",
            "storage.modifier.async",
            "keyword.control",
            "keyword.operator.new"
          ],
          "settings": {
            "foreground": "#C678DD"
          }
        },
        {
          "scope": [
            "entity.other.attribute-name"
          ],
          "settings": {
            "foreground": "#d19a66",
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "support.type.builtin",
            "support.type.primitive"
          ],
          "settings": {
            "foreground": "#E5C07B"
          }
        },
        {
          "scope": [
            "variable.other.readwrite.alias",
            "variable.other.property",
            "variable.other.constant",
            "variable.other.object"
          ],
          "settings": {
            "foreground": "#43b6b9"
          }
        }
      ]
    }
  },
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 1000,
  "eslint.format.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "git.confirmSync": false,
  "haskell.manageHLS": "GHCup",
  "highlight-matching-tag.styles": {
    "opening": {
      "name": {
        "custom": {
          "borderColor": "yellow",
          "borderStyle": "solid",
          "borderWidth": "0 0 1px 0",
        }
      }
    }
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "latex-workshop.view.pdf.viewer": "tab",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "notebook.lineNumbers": "on",
  "prolog.executablePath": "C:\\Program Files\\swipl\\bin\\swipl",
  "prolog.format.tabSize": 2,
  "prolog.linter.run": "onType",
  "python.terminal.executeInFileDir": true,
  "python.testing.pytestEnabled": true,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": " CaskaydiaCove NF, Consolas ligaturized v2, Consolas",
  "typescript.autoClosingTags": true,
  "typescript.implementationsCodeLens.enabled": true,
  "window.autoDetectHighContrast": false,
  "window.commandCenter": true,
  "window.menuBarVisibility": "compact",
  "window.title": "${activeFolderMedium}\\${activeEditorShort}",
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "One Monokai",
  "workbench.colorCustomizations": {
    "activityBar.background": "#181819",
    "editor.background": "#181819",
    "editor.lineHighlightBackground": "#181819",
    "editor.lineHighlightBorder": "#333333",
    "editor.selectionBackground": "#3E4451",
    "editor.selectionHighlightBackground": "#333333",
    "editorBracketHighlight.foreground1": "#c172d9",
    "editorBracketHighlight.foreground2": "#5caeef",
    "editorBracketHighlight.foreground3": "#dfb976",
    "editorCursor.foreground": "#3178C6",
    "editorGroup.emptyBackground": "#181819",
    "editorGroupHeader.tabsBackground": "#202022",
    "editorHoverWidget.background": "#202022",
    "editorSuggestWidget.background": "#202022",
    "editorSuggestWidget.selectedBackground": "#333333",
    "input.background": "#181819",
    "list.activeSelectionBackground": "#181819",
    "list.activeSelectionForeground": "#b8b6b6",
    "list.focusBackground": "#181819",
    "list.focusOutline": "#333333",
    "list.inactiveSelectionBackground": "#181819",
    "list.inactiveSelectionForeground": "#b8b6b6",
    "menu.background": "#202022",
    "menu.selectionBackground": "#333333",
    "quickInput.background": "#202022",
    "quickInputList.focusBackground": "#333333",
    "scrollbarSlider.background": "#202022",
    "sideBar.background": "#181819",
    "sideBar.border": "#181819",
    "statusBar.background": "#181819",
    "tab.activeBackground": "#181819",
    "tab.border": "#202022",
    "tab.inactiveBackground": "#202022",
    "tab.inactiveForeground": "#b8b6b6",
    "terminal.background": "#181819",
    "terminalCursor.foreground": "#3178C6",
    "titleBar.activeBackground": "#202022",
    "titleBar.inactiveBackground": "#202022",
    "toolbar.activeBackground": "#181819",
    "tree.indentGuidesStroke": "#3E4451",
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": true,
  "workbench.editorAssociations": {
    "*.pdf": "latex-workshop-pdf-hook"
  },
  "workbench.iconTheme": "moxer-icons",
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": false,
  "workbench.tree.renderIndentGuides": "always",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideTabs": false,
}
